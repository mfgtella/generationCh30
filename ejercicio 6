let number = [3, 6, 12, 5, 100, 1];

function bubbleForm(arr){
    for (let i=0; i < arr.length; i++){
        for (let j=0; j < arr.length; i++){
            if (arr[j] > arr[j + 1]){
              let temp = arr[j];
                arr[j] = arr [j +1];
                arr [j + 1] = temp;
            }
        }
    }
    console.log(arr);
}
bubbleForm(number);
